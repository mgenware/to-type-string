{
  "name": "to-type-string",
  "version": "1.1.0",
  "description": "Get type description of a JavaScript variable",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/to-type-string",
  "bugs": "https://github.com/mgenware/to-type-string/issues",
  "keywords": [
    "gettype",
    "typeof",
    "debug"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/to-type-string.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "jest",
    "build": "yarn test",
    "compile": "tsc -b tests",
    "dev": "yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "clean": "rimraf dist && rimraf dist_tests"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.map"
  ],
  "jest": {
    "testRegex": "/dist_tests/.*(\\.|/)(test|spec)\\.jsx?$"
  },
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
